                                            <div class="accordion__item">
                                                <div class="accordion__header rounded-lg" data-toggle="collapse" data-target="#default_collapseOne">
                                                    <span class="accordion__header--text">Product Campaign</span>
                                                    <span class="accordion__header--indicator"></span>
                                                </div>
                                                <div id="default_collapseOne" class="collapse accordion__body show" data-parent="#accordion-one">
                                                    <div class="accordion__body--text">
                                                        @foreach (var item in ViewBag.lstAllCampaigns)
                                                        {
                                                            if (item.campaign_category_id == 1 && item.campaign_id == 1)
                                                            {
                                                                <div class="campaign-audio col-md-12">
                                                                    <div class="compaign-row align-items-center p-sm-4 p-3 row sp16 mx-0 mb-2">
                                                                        <div class="my-2 col-xl-4 col-xxl-4 col-lg-4 col-md-3 col-sm-6">
                                                                            <div class="media align-items-center">
                                                                                @{ string[] parts = item.campaign_master_images.Split(',');
                                                                                    <img class="align-self-center mr-3 img-fluid" src="~/Content/images/campaign/@parts[0]" alt=""> }


                                                                            </div>
                                                                        </div>
                                                                        <div class="col-xl-8 col-xxl-8 col-lg-8 col-md-9 col-sm-6 px-3">
                                                                            <div class="media-body">
                                                                                <h3 class="fs-20"><a class="text-black" href="#">@item.campaign_master_creative_caption</a></h3>
                                                                                @{
                                                                                    decimal adsNo = Convert.ToDecimal(item.campaign_master_id);
                                                                                    adsNo = 100000 + adsNo;
                                                                                }
                                                                                <p class="text-primary mb-1"><i class="fa fa-list" style="color:red"></i> @("#ADS" + @adsNo.ToString())</p>
                                                                                <p class="text-primary mb-1"> <i class="fa fa-calendar" style="color:red"></i> <strong>Published on</strong> @item.campaign_master_start_date.ToString("dd MMM yyyy")</p>
                                                                                <p class="text-primary mb-1">
                                                                                    <i class="fa fa-check-square" style="color:red"></i><strong> Status: </strong>
                                                                                    @if (item.camapaign_master_status == "Pause")
                                                                                    {
                                                                                        <spam class="badge badge-rounded badge-warning">Pause</spam>
                                                                                    }
                                                                                    else if (System.DateTime.UtcNow.AddMinutes(330) >= item.campaign_master_start_date && System.DateTime.UtcNow.AddMinutes(330) <= item.campaign_master_end_date)
                                                                                    {
                                                                                        <spam class="badge badge-rounded badge-success">Ongoning</spam>
                                                                                    }
                                                                                    else if (System.DateTime.UtcNow.AddMinutes(330) > item.campaign_master_end_date)
