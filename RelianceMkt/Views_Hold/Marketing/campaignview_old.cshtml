                            <li class=" nav-item">
                                <a href="#navpills2-1" class="nav-link active" data-toggle="tab" aria-expanded="false">Sales</a>
                            </li>
                            <li class="nav-item">
                                <a href="#navpills2-2" class="nav-link" data-toggle="tab" aria-expanded="false">Recruitment</a>
                            </li>
